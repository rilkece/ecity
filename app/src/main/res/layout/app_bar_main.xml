            app:fab_colorNormal="#D50000"
            app:fab_colorPressed="#EF9A9A"
            app:fab_label="@string/label_saude"
            app:fab_size="mini"


            />

        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/fab_educ"
            android:layout_width="wrap_content"
