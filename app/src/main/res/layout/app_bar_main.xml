            app:fab_colorNormal="#1A237E"
            app:fab_colorPressed="#9FA8DA"
            app:fab_label="@string/label_educacao"
            app:fab_size="mini"


            />

        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/fab_seguranca"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/policia_24"
            app:fab_colorDisabled="@color/colorPrimary"
            app:fab_colorNormal="#D84315"
            app:fab_colorPressed="#FFAB91"
            app:fab_label="@string/label_seguranca"
            app:fab_size="mini"


            />

        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/fab_lazer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/balanco_24"
            app:fab_colorDisabled="@color/colorPrimary"
            app:fab_colorNormal="#212121"
            app:fab_colorPressed="#E0E0E0"
            app:fab_label="@string/label_lazer"
            app:fab_size="mini"


            />

        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/fab_meio_ambiente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/folha_24"
            app:fab_colorDisabled="@color/colorPrimary"
            app:fab_colorNormal="#2E7D32"
            app:fab_colorPressed="#A5D6A7"
            app:fab_label="@string/label_ambiente"
            app:fab_size="mini"


            />

    </com.github.clans.fab.FloatingActionMenu>

    <com.github.clans.fab.FloatingActionMenu
        android:id="@+id/marcadores"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginRight="8dp"
